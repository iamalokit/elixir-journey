  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="apple-touch-icon" sizes="57x57" href="<%= Routes.static_path(@conn, "/favicon/apple-icon-57x57.png") %>">
